{
  "href": {
    "keys": "http://soundcloud.com/you/apps/new",
    "docs": "http://developers.soundcloud.com/docs/api/reference",
    "apps": "http://soundcloud.com/you/apps",
    "provider": "http://soundcloud.com"
  },
  "oauth2": {
    "authorize": {
      "url": "https://soundcloud.com/connect",
      "query": {
        "scope": "{scope}",
        "state": "{{state}}",
        "redirect_uri": "{{callback}}",
        "response_type": "code",
        "client_id": "{client_id}"
      }
    },
    "access_token": {
      "url": "https://api.soundcloud.com/oauth2/token",
      "method": "post"
    },
    "request": {
      "query": {
        "oauth_token": "{{token}}"
      }
    },
    "parameters": {
      "client_secret": "string",
      "client_id": "string",
      "scope": {
        "values": {
          "non-expiring": "To obtain a non expiring access token"
        },
        "cardinality": "*",
        "separator": ",",
        "type": "string"
      }
    }
  },
  "api_url": "https://api.soundcloud.com/",
  "name": "Soundcloud"
}