{if("items" ne "")}
<div align="center">
<table align="center" width="60%">
	<thead>
		<th>#</th>
		<th width="20%">{t(Name)}</th>
		<th>{t(Login)}</th>
		<th>{t(Group)}</th>
		<th>{t(Add Date)}</th>
		<th>{t(URL after login)}</th>
		<th>{t(Active)}</th>
		<th width="5%">{t(Action)}</th>
	</thead>
	<tbody>
{foreach("items")}
		<tr class="{#.bg_class}">
			<td>{#.id}</td>
			<td>{#.first_name} {#.last_name}</td>
			<td>{#.login}</td>
			<td><a href="{#.edit_group_link}">{#.group_name}</a></td>
			<td>{#.add_date}</td>
			<td>{#.go_after_login}</td>
			<td>
	{if("#.active_link" ne "")}<a href="{#.active_link}" class="change_active">{/if}
	{if("#.active" eq 1)}<span class="positive">{t(ACTIVE)}</span>{else}<span class="negative">{t(INACTIVE)}</span>{/if}
	{if("#.active_link" ne "")}</a>{/if}
			</td>
			<td nowrap="nowrap">
	{if("#.edit_link" ne "")}<a href="{#.edit_link}"><img height="16" width="16" border="0" src="images/links_edit.gif" alt="{t(Edit)}" /></a>&nbsp;{/if}
	{if("#.admin_auth_link" ne "")}<small>[<a href="{#.admin_auth_link}">{t(log auth)}</a>]</small>&nbsp;{/if}
	{if("#.delete_link" ne "")}<a href="{#.delete_link}" onclick="return confirm('{t(Are you sure)}?');"><img height="16" width="16" border="0" src="images/links_delete.gif" alt="{t(Delete)}" /></a>&nbsp;{/if}
			</td>
		</tr>
{/foreach}
	</tbody>
</table>
</div>
{/if}

{if("failed_log_link" ne "")}<br /><div align="left"><a href="{failed_log_link}">Failed auth log</a></div>{/if}

{if("items" eq "")}<div align="center">{t(No admin users yet)}</div>{/if}

{if("pages" ne "")}<br /><div align="center">{t(Pages)}: {pages}</div>{/if}

{if("add_link" ne "")}<br /><div align="center"><a href="{add_link}">Add</a></div>{/if}