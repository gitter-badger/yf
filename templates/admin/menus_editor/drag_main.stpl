<form action="{form_action}" method="post" id="draggable_form">

<div class="controls">
	<input type="submit" class="btn btn-primary btn-mini" value="{t(Save)}">
	<a href="{add_link}" class="btn btn-mini ajax_add">{t(Add)}</a>
	<a href="{back_link}" class="btn btn-mini">{t(Back)}</a>
</div>

<ul class="draggable_menu">
{items}
</ul>

</form>

{include("draggable_menu_js")}
