	<script>
	try {
		var ng_app_admin = angular.module( '{ng_app}', [
			'ngAnimate'
			, 'ngSanitize'
			, 'ngResource'
			, 'ngTouch'
			{{-- , 'ui.bootstrap' --}}
			, 'ui.select2'
			, 'mgcrea.ngStrap'
		]);
		ng_app_admin.config( function( $datepickerProvider ) {
			angular.extend( $datepickerProvider.defaults, {
				dateFormat: 'dd.MM.yyyy'
				, autoclose: 1
				, startWeek: 1
				, startView: 2
			});
		});

	} catch (e) { console.error(e); }
	</script>
