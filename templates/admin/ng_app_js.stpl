	<script type="text/javascript">
	try {
		var ng_app_admin = angular.module( '{ng_app}', [
			'ngAnimate'
			, 'ngSanitize'
			, 'ngResource'
			, 'ngTouch'
			, 'mgcrea.ngStrap'
		])

		.config( function( $datepickerProvider ) {
			angular.extend( $datepickerProvider.defaults, {
				dateFormat: 'dd.MM.yyyy'
				, autoclose: 1
				, startWeek: 1
				, startView: 2
			});
		});
	} catch (e) { console.error(e); }
	</script>
