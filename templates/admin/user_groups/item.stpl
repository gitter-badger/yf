	<tr>
		<td>{name}</td>
		<td>{num_members}</td>
		<td>{go_after_login}</td>
		<td><a href="{active_link}" class="change_active">{if("active" eq 1)}<span class="label label-success">{t(ACTIVE)}</span>{/if}{if("active" eq 0)}<span class="label label-warning">{t(INACTIVE)}</span>{/if}</a></td>
		<td>
			<a href="{edit_link}" title="{t(Edit)}"><i class="icon-edit"></i></a>&nbsp;
			<a href="{delete_link}" title="{t(Delete)}" onclick="return confirm('{t(Are you sure)}?');"><i class="icon-trash"></i></a>		
		</td>
	</tr>